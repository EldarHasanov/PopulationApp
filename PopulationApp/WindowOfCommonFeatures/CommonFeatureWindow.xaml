        <StackPanel Orientation="Horizontal"  >
            <Button Content="Вийти" HorizontalAlignment="Left" Margin="50,360,0,0" VerticalAlignment="Top" FontSize="20" Click="Button_Click_Exsit" Width="150"/>
            <Button Content="Змінити пароль" HorizontalAlignment="Left" Margin="-150,320,0,0" VerticalAlignment="Top" FontSize="20" Click="Button_Click_Change" Width="150"/>
            <Button Content="На головну" HorizontalAlignment="Left" Margin="-150,280,0,0" VerticalAlignment="Top" FontSize="20" Click="Button_Click_Main" Width="150"/>
        </StackPanel>
        <Frame Name="Main" NavigationUIVisibility="Hidden">

        </Frame>
    </Grid>
</Window>
