<Window x:Class="PopulationApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PopulationApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox HorizontalAlignment="Left" Margin="350,280,0,0" VerticalAlignment="Top" Width="400" FontSize="20" Name="Login"/>
        <PasswordBox HorizontalAlignment="Left" Margin="350,320,0,0" VerticalAlignment="Top" Width="400" FontSize="20" Name="Password"/>
        <Button Content="Авторизуватися" HorizontalAlignment="Left" Margin="350,360,0,0" VerticalAlignment="Top" FontSize="20" Width="400" IsDefault="True" Click="SingInButton_Click"/>
        <TextBlock HorizontalAlignment="Left" Margin="100,75,0,0" Text="Population App" TextAlignment="Center" TextWrapping="Wrap" VerticalAlignment="Top" Name="AAA" Width="600" FontSize="35"/>
        <TextBlock HorizontalAlignment="Left" Margin="283,293,0,0" Text="Логін" TextWrapping="Wrap" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="283,333,0,0" Text="Пароль" TextWrapping="Wrap" VerticalAlignment="Top"/>

    </Grid>
</Window>
